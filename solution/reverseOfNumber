public class Solution {
    public int isPalindrome(int A) {
        int N=A;  //A became 0 after while loop 
        if(A<0)
        {return 0;}
       int sum=0;
       while(A>0)
       {
           sum=sum*10+A%10;
           A=A/10;
       }
      // System.out.println(N+ " "+sum);
       if(sum==N)
       {return 1;}
        return 0;
    }
}
